# FWC API Configuration
FWC_API_BASE_URL=https://api.fwc.gov.au/api/v1
FWC_API_KEY=your_api_key_here

# MS SQL Configuration (existing container)
MSSQL_CONNECTION_STRING=mssql+pyodbc://sa:YourPassword@localhost:1433/etl_pipeline?driver=ODBC+Driver+17+for+SQL+Server
# Or individual params:
MSSQL_HOST=localhost
MSSQL_PORT=1433
MSSQL_DATABASE=etl_pipeline
MSSQL_USER=sa
MSSQL_PASSWORD=YourPassword

# For Docker containers connecting to host's MS SQL
# MSSQL_HOST=host.docker.internal

# SQLite for state management
SQLITE_DATABASE_PATH=./data/state.db

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
# CORS_ORIGINS=http://localhost:3000,http://localhost:8081  # Comma-separated, or * for all
CORS_ORIGINS=*

# UI Configuration
NEXT_PUBLIC_API_URL=http://localhost:8081/api
NEXT_PUBLIC_WS_URL=ws://localhost:8081/ws

# Scheduler
ETL_SCHEDULE_ENABLED=true
ETL_CRON_EXPRESSION=0 2 * * *

# Logging
LOG_LEVEL=INFO
LOG_FILE_PATH=./logs/etl.log
